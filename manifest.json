{
  "manifest_version": 3,
  "name": "SVG to Icon",
  "version": "1.0.0",
  "description": "Convert SVG to various icon sizes",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}